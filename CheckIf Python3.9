#!/bin/bash

# Get the current python3 version
python_version=$(python3 --version | awk '{print $2}')

# Check if the version is 3.9
if [[ "$python_version" == "3.9"* ]]; then
    echo "The current Python version is 3.9"
else
    echo "The current Python version is $python_version. Exiting."
    exit 1
fi
